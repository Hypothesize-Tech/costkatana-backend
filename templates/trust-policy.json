{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "CostKatanaTrust",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::COSTKATANA_ACCOUNT_ID:root"
      },
      "Action": "sts:AssumeRole",
      "Condition": {
        "StringEquals": {
          "sts:ExternalId": "YOUR_EXTERNAL_ID"
        }
      }
    }
  ]
}
