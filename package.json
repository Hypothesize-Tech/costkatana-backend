{
  "name": "ai-cost-optimizer-backend",
  "version": "2.0.0",
  "description": "AI-powered cost optimization backend for Cost Katana - Intelligent monitoring, personalized recommendations, predictive analytics, and seamless onboarding for AI usage optimization",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec \"node --max-old-space-size=4096 --expose-gc -r ts-node/register -r ./src/observability/register.ts src/server.ts\"",
    "build": "tsc",
    "start": "node --max-old-space-size=4096 --expose-gc -r ./dist/observability/register.js dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "otel:install": "bash scripts/otel-install.sh",
    "otel:run": "bash scripts/otel-run.sh",
    "otel:stop": "bash scripts/otel-stop.sh",
    "otel:restart": "bash scripts/otel-restart.sh",
    "telemetry:verify": "node scripts/verify-telemetry.js",
    "telemetry:validate": "ts-node -e \"import('./src/observability/telemetryValidator').then(m => m.validateTelemetryConfig())\"",
    "telemetry:test": "ts-node -e \"import('./src/observability/telemetryValidator').then(m => m.generateTestTelemetry())\""
  },
  "keywords": [
    "ai-cost-optimization",
    "intelligent-monitoring",
    "personalized-recommendations",
    "predictive-analytics",
    "cost-intelligence",
    "magic-link-onboarding",
    "aws-bedrock",
    "chatgpt-integration",
    "ai-coaching",
    "usage-forecasting",
    "openai",
    "anthropic",
    "cost-tracking"
  ],
  "author": "ABDUL SAGHEER",
  "license": "MIT",
